import streamlit as st
from streamlit_folium import st_folium
import folium

st.title("🌿 Simple Interactive Map")

# สร้าง map โดยไม่กำหนดพิกัดเฉพาะ (center default)
m = folium.Map(zoom_start=2, tiles="OpenStreetMap")

# แสดง map ใน Streamlit
st_data = st_folium(m, width=700, height=500)
