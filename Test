print("Pixel indices row:", row_min, "-", row_max)
print("Pixel indices col:", col_min, "-", col_max)

pixels_crop = bands[:, row_min:row_max, col_min:col_max]
print("Cropped shape:", pixels_crop.shape)

row_min = max(0, row_min)
row_max = min(src.height, row_max)
col_min = max(0, col_min)
col_max = min(src.width, col_max)
